    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.47.0",
    "postcss": "^8.5.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.0.3",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^3.0.6"
  },
  "overrides": {
    "esbuild": "^0.25.2"
  }
