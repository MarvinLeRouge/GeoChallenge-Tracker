# === Nouvelles variables pour le frontend unifi√© ===
DOCKER_TARGET=dev
FRONTEND_PORT=5173
FRONTEND_INTERNAL_PORT=5173
CHOKIDAR_USEPOLLING=true

# MongoDB
MONGODB_USER=your_mongodb_username
MONGODB_PASSWORD=your_mongodb_password
MONGODB_URI_TPL=mongodb+srv://[[MONGODB_USER]]:[[MONGODB_PASSWORD]]@cluster0.yourcluster.mongodb.net/?retryWrites=true&w=majority&appName=Cluster0
MONGODB_DB=your_database_name

# FastAPI settings
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=true

# Seeding
SEED_ON_STARTUP=true

# Auth
JWT_SECRET_KEY=your_jwt_secret_key_here_generate_a_secure_random_one
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=60

# Frontend allowed origins
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:8081

# Uploads
UPLOAD_FOLDER=./uploads
LOG_LEVEL=debug

# SEEDING DATA
ADMIN_USERNAME=your_admin_username
ADMIN_EMAIL=your_admin_email@example.com
ADMIN_PASSWORD=your_secure_admin_password

# MAIL
MAIL_FROM=Your App Name <no-reply@yourdomain.com>
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USERNAME=your_smtp_username
SMTP_PASSWORD=your_smtp_password

# ELEVATION
ELEVATION_PROVIDER=opentopo
ELEVATION_PROVIDER_ENDPOINT=https://api.opentopodata.org/v1/mapzen
ELEVATION_PROVIDER_MAX_POINTS_PER_REQ=100
ELEVATION_PROVIDER_RATE_DELAY_S=1
ELEVATION_ENABLED=1

# UPLOAD LIMITS
ONE_MB=1048576
MAX_UPLOAD_MB=20

#Vite
VITE_API_URL=/api
VITE_TILE_URL=/tiles/{z}/{x}/{y}.png

TEST=placeholder_value